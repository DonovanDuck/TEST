	</insert>
	<insert id="insertClassAfterModify" parameterType="cn.edu.tit.bean.TurnClassAfterModify">
		INSERT INTO
		turn_class_after_modify
		(id,
		task_id,
		plan_stage,
		demand_stage,
		design_stage,
		arithmetic_stage,
		test_stage,
		author_id,
		author_name,
		author_picture,
		accessory_path,
		upload_time,
		content,
		team_id,
		stage
		)
		VALUES
		(#{id},
		#{taskId},
		#{planStage},
		#{demandStage},
		#{designStage},
		#{arithmeticStage},
		#{testStage},
		#{authorId},
		#{authorName},
		#{authorPicture},
		#{accessoryPath},
		#{uploadTime},
		#{content},
