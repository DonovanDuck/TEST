		stage
		)
		VALUES
		(#{id},
		#{taskId},
		#{planStage},
		#{demandStage},
		#{designStage},
		#{arithmeticStage},
		#{testStage},
		#{authorId},
		#{authorName},
		#{authorPicture},
		#{accessoryPath},
		#{uploadTime},
		#{content},
